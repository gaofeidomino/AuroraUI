{
  "name": "aurora-ui-plus",
  "version": "0.1.0",
  "description": "A modern Vue 3 UI component library built with Tailwind CSS",
  "keywords": [
    "vue",
    "vue3",
    "ui",
    "components",
    "tailwind",
    "typescript"
  ],
  "author": "gaofei",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gaofei/AuroraUI.git"
  },
  "homepage": "https://github.com/gaofei/AuroraUI#readme",
  "bugs": {
    "url": "https://github.com/gaofei/AuroraUI/issues"
  },
  "private": false,
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/aurora-ui-plus.es.js",
      "require": "./dist/aurora-ui-plus.umd.js"
    },
    "./style": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:lib": "BUILD_LIB=true vite build && npm run build:types",
    "build:types": "vue-tsc --project tsconfig.build.json",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "prepublishOnly": "npm run build:lib"
  },
  "peerDependencies": {
    "vue": ">=3.2.0"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.11",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "pinia": "^3.0.3",
    "reka-ui": "^2.4.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.3.6",
    "unplugin-vue-components": "^28.8.0",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^22.16.5",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.31.0",
    "eslint-plugin-vue": "~10.3.0",
    "jiti": "^2.4.2",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.6.2",
    "typescript": "~5.8.0",
    "vite": "^7.0.6",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vue-tsc": "^3.0.4"
  }
}