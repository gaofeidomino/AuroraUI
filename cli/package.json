{
    "name": "aurora-ui-cli",
    "version": "1.0.2",
    "description": "CLI tool for adding AuroraUI components to your project",
    "type": "module",
    "main": "src/index.ts",
    "bin": {
        "aurora-ui": "./src/index.ts"
    },
    "scripts": {
        "build": "tsc",
        "dev": "tsx src/index.ts",
        "test": "vitest",
        "test:run": "vitest run",
        "test:ui": "vitest --ui",
        "test:coverage": "vitest run --coverage"
    },
    "dependencies": {
        "commander": "^12.1.0"
    },
    "devDependencies": {
        "@types/node": "^22.16.5",
        "@vitest/coverage-v8": "^3.2.4",
        "@vitest/ui": "^3.2.4",
        "pathe": "^1.1.2",
        "tsx": "^4.19.2",
        "typescript": "~5.8.0",
        "vitest": "^3.2.4"
    },
    "engines": {
        "node": "^20.19.0 || >=22.12.0"
    }
}